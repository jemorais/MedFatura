{
  "name": "medfatura-app",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "dependencies": {
    "@hono/node-server": "^1.12.0",
    "@hono/zod-validator": "^0.5.0",
    "@sendgrid/mail": "^8.1.5",
    "@supabase/supabase-js": "^2.55.0",
    "@types/sqlite3": "^3.1.11",
    "bcryptjs": "^2.4.3",
    "dotenv": "^17.2.1",
    "googleapis": "^144.0.0",
    "hono": "4.7.7",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.510.0",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-router": "^7.5.3",
    "sqlite3": "^5.1.7",
    "zod": "^3.24.3"
  },
  "devDependencies": {
    "@eslint/js": "9.25.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/dotenv": "^6.1.1",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/node": "22.14.1",
    "@types/react": "19.0.10",
    "@types/react-dom": "19.0.4",
    "@vitejs/plugin-react": "4.4.1",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.0.1",
    "eslint": "^9.32.0",
    "eslint-plugin-react-hooks": "5.2.0",
    "eslint-plugin-react-refresh": "0.4.19",
    "globals": "15.15.0",
    "postcss": "^8.5.3",
    "tailwindcss": "^3.4.17",
    "tsx": "^4.19.1",
    "typescript": "5.8.3",
    "typescript-eslint": "8.31.0",
    "vite": "^6.3.5",
    "wrangler": "^3.78.12"
  },
  "scripts": {
    "build": "node build.js",
    "build:react": "vite build",
    "build:worker": "tsc -p tsconfig.worker.json",
    "dev": "concurrently --kill-others \"npm run dev:server\" \"npm run dev:client\"",
    "dev:client": "vite --port 0",
    "dev:server": "tsx watch server.ts",
    "lint": "eslint .",
    "preview": "vite preview",
    "start": "node server.js",
    "migrate": "tsx migrate.ts",
    "clean": "taskkill /f /im node.exe 2>nul || echo 'No Node processes to kill'",
    "deploy": "wrangler deploy",
    "deploy:staging": "wrangler deploy --env staging",
    "deploy:production": "wrangler deploy --env production",
    "wrangler:login": "wrangler login",
    "wrangler:tail": "wrangler tail"
  }
}
